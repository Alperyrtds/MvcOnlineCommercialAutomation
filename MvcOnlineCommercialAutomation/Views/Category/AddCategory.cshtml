@model MvcOnlineCommercialAutomation.Models.Class.Category
@{
    ViewBag.Title = "AddCategory";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Kategori Ekleme Sayfası</h2>
<br />
<form  method="post" class="form-group">
    @Html.Label("Kategori Adı")
    @Html.TextBoxFor(x=>x.CategoryName,new {@class="form-control" })
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>


